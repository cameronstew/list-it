<div class="container">
<div class="page-header">

  <div class="pull-right">
    <%= link_to "Create List", new_list_path, class: 'btn btn-primary' %>
  </div>
  <h1>My lists</h1>
</div>
<div class="my-lists">

  <% @lists.each do |list| %>
  <p>
    <%= link_to list.name, list_path(list) %>
  </p>
  <% end %>
</div>
</div>
